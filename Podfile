  use_frameworks!

  pod 'Firebase/Database'
  pod 'Firebase/Core'
  pod 'Firebase/Auth'
  
  pod 'PromisesSwift'
  
  pod 'AppLocationManager' , :path => "."
  pod 'MyRunningAppMapView' , :path => "."
  
end
